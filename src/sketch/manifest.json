      "name": "Atlassian Design Website",
      "identifier": "link-design-site",
      "script": "handler/menu/links.js",
      "handler": "linkAtlassianDesignSite"
    },
    {
      "name": "Atlaskit Website",
      "identifier": "link-atlaskit-site",
      "script": "handler/menu/links.js",
      "handler": "linkAtlaskitSite"
